<!DOCTYPE html>
<html>
  <head>
    <title>Title</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <%- JSON.stringify(records) %>
    <table>
      <tr>
        <th>id</th>
        <th>title</th>
        <th>description</th>
        <th>price</th>
        <th>stock</th>
      </tr>
      <tbody>
        <% records.forEach((record) => { %>
          <tr>
            <td><%= record.id %></td>
            <td><%= record.title %></td>
            <td><%= record.description %></td>
            <td><%= record.price %></td>
            <td><%= record.stock %></td>
          </tr>
        <% }) %>
      </tbody>
      </table>
  </body>
</html>
